@using BE
@using MyServiceDesk.Enums
@{
    Layout = null;
    var modulos = Session["modulos"] as List<BEModulo> ?? new List<BEModulo>();
    var operaciones = Session["operaciones"] as List<BE.BEOperacion> ?? new List<BE.BEOperacion>();
    BEUsuario usuario = ((Session["usuario"] as BEUsuario) != null) ? Session["usuario"] as BEUsuario : new BEUsuario() { RolesUsuario = new List<BERol>() };

    var ruta = ViewContext.RouteData.Values;
}
<!DOCTYPE html>
